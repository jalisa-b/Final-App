<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/green_light"
    tools:context=".MenuFragment">

    <ImageButton
        android:id="@+id/mosquito"
        android:layout_width="64dp"
        android:layout_height="65dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="124dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a mosquito"
        android:scaleType="fitCenter"
        android:src="@drawable/mosquito"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/cockroach"
        android:layout_width="68dp"
        android:layout_height="67dp"
        android:layout_marginTop="124dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a cockroach"
        android:scaleType="fitCenter"
        android:src="@drawable/cockroach"
        app:layout_constraintEnd_toStartOf="@+id/bee"
        app:layout_constraintHorizontal_bias="0.475"
        app:layout_constraintStart_toEndOf="@+id/mosquito"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/bee"
        android:layout_width="68dp"
        android:layout_height="69dp"
        android:layout_marginTop="124dp"
        android:layout_marginEnd="48dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a bee"
        android:scaleType="fitCenter"
        android:src="@drawable/bee"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageButton
        android:id="@+id/fly"
        android:layout_width="68dp"
        android:layout_height="68dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="52dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a fly"
        android:scaleType="fitCenter"
        android:src="@drawable/fly"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/beeText" />

    <ImageButton
        android:id="@+id/ant"
        android:layout_width="68dp"
        android:layout_height="66dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a ant"
        android:scaleType="fitCenter"
        android:src="@drawable/ant"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mosquitoText" />

    <ImageButton
        android:id="@+id/cricket"
        android:layout_width="63dp"
        android:layout_height="59dp"
        android:layout_marginTop="24dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a cricket"
        android:scaleType="fitCenter"
        android:src="@drawable/cricket"
        app:layout_constraintEnd_toStartOf="@+id/fly"
        app:layout_constraintHorizontal_bias="0.486"
        app:layout_constraintStart_toEndOf="@+id/ant"
        app:layout_constraintTop_toBottomOf="@+id/cockroachText" />

    <ImageButton
        android:id="@+id/beetle"
        android:layout_width="71dp"
        android:layout_height="67dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="48dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a beetle"
        android:scaleType="fitCenter"
        android:src="@drawable/beetle"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/flyText" />

    <ImageButton
        android:id="@+id/spider"
        android:layout_width="75dp"
        android:layout_height="73dp"
        android:layout_marginStart="52dp"
        android:layout_marginTop="20dp"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a beetle"
        android:scaleType="fitCenter"
        android:src="@drawable/spider"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/snakeText" />

    <ImageButton
        android:id="@+id/snake"
        android:layout_width="72dp"
        android:layout_height="71dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="20dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a snake"
        android:scaleType="fitCenter"
        android:src="@drawable/snake"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/antText" />

    <ImageButton
        android:id="@+id/wasp"
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:layout_marginTop="16dp"
        android:adjustViewBounds="true"
        android:background="@color/green_light"
        android:contentDescription="Clickable picture of a wasp"
        android:scaleType="fitCenter"
        android:src="@drawable/wasp"
        app:layout_constraintEnd_toStartOf="@+id/beetle"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toEndOf="@+id/snake"
        app:layout_constraintTop_toBottomOf="@+id/cricketText" />

    <ImageButton
        android:id="@+id/goToCustom"
        android:layout_width="67dp"
        android:layout_height="70dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="48dp"
        android:alpha=".6"
        android:backgroundTint="@color/green"
        android:src="@android:drawable/ic_menu_add"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/beetleText"
        app:tint="@color/black" />

    <ImageButton
        android:id="@+id/returnToDisclaimer"
        android:layout_width="66dp"
        android:layout_height="53dp"
        android:layout_marginStart="48dp"
        android:layout_marginTop="20dp"
        android:backgroundTint="@color/blue"
        android:src="?attr/actionModeCloseDrawable"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spiderText" />

    <Button
        android:id="@+id/tipsButton"
        android:layout_width="69dp"
        android:layout_height="49dp"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="48dp"
        android:layout_marginBottom="80dp"
        android:backgroundTint="@color/blue"
        android:text="Tips"
        android:textColor="@color/purple_700"
        app:iconTint="@color/black"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/customText"
        app:layout_constraintVertical_bias="0.0" />

    <TextView
        android:id="@+id/mosquitoText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="52dp"
        android:layout_marginTop="4dp"
        android:text="MOSQUITO"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mosquito" />

    <TextView
        android:id="@+id/cockroachText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="28dp"
        android:layout_marginTop="4dp"
        android:text="COCKROACH"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/mosquitoText"
        app:layout_constraintTop_toBottomOf="@+id/cockroach" />

    <TextView
        android:id="@+id/beeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="4dp"
        android:text="BEE"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.19"
        app:layout_constraintStart_toEndOf="@+id/cockroachText"
        app:layout_constraintTop_toBottomOf="@+id/bee" />

    <TextView
        android:id="@+id/antText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="80dp"
        android:layout_marginTop="4dp"
        android:text="ANT"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/ant" />

    <TextView
        android:id="@+id/cricketText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="4dp"
        android:text="CRICKET"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/antText"
        app:layout_constraintTop_toBottomOf="@+id/cricket" />

    <TextView
        android:id="@+id/flyText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="40dp"
        android:layout_marginTop="4dp"
        android:text="FLY"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.346"
        app:layout_constraintStart_toEndOf="@+id/cricketText"
        app:layout_constraintTop_toBottomOf="@+id/fly" />

    <TextView
        android:id="@+id/snakeText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="64dp"
        android:layout_marginTop="4dp"
        android:text="SNAKE"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/snake" />

    <TextView
        android:id="@+id/waspText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="4dp"
        android:text="WASP"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/snakeText"
        app:layout_constraintTop_toBottomOf="@+id/wasp" />

    <TextView
        android:id="@+id/beetleText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="68dp"
        android:layout_marginTop="4dp"
        android:text="BEETLE"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toEndOf="@+id/waspText"
        app:layout_constraintTop_toBottomOf="@+id/beetle" />

    <TextView
        android:id="@+id/spiderText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="60dp"
        android:layout_marginTop="4dp"
        android:text="SPIDER"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/spider" />

    <TextView
        android:id="@+id/customText"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="48dp"
        android:text="CUSTOM"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/goToCustom" />

    <Button
        android:id="@+id/goToWeather"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="96dp"
        android:text="Get Weather"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/tipsButton"
        app:layout_constraintHorizontal_bias="0.538"
        app:layout_constraintStart_toEndOf="@+id/returnToDisclaimer" />

    <TextView
        android:id="@+id/bugHeader"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="23dp"
        android:layout_marginTop="85dp"
        android:layout_marginEnd="24dp"
        android:text="Click a bug image to play ultrasonic repellent!"
        android:textAlignment="center"
        android:textSize="18sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>

